<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="FIT5042Final-ejbPU" transaction-type="JTA">
    <jta-data-source>jdbc/fit5042final</jta-data-source>
    <class>entities.UserGroups</class>
    <class>entities.Users</class>
    <class>entities.Cart</class>
    <class>entities.Genre</class>
    <class>entities.Item</class>
    <class>entities.Orders</class>
    <class>entities.Status</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
